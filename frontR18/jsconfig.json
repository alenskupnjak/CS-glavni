{
	"compilerOptions": {
		"experimentalDecorators": true,
		"baseUrl": "src",
		"paths": {
			"@app/*": ["./src/@app/*"],
			"components/*": ["./src/components"],
			"@data": ["./src/@data"],
			"app/stores/*": ["./src/app/store/*"],
			"app/components/*": ["./src/app/components/*"]
		}
	},
	"include": ["src/**/*"]
}
