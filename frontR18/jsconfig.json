{
	"compilerOptions": {
		"baseUrl": "src",
		"paths": {
			"@app/*": ["./src/@app/*"],
			"@history*": ["./src/@history"],
			"@mock-api": ["./src/@mock-api"],
			"app/store/*": ["./src/app/store/*"],
			"app/shared-components/*": ["./src/app/shared-components/*"],
			"app/configs/*": ["./src/app/configs/*"],
			"app/theme-layouts/*": ["./src/app/theme-layouts/*"],
			"app/AppContext": ["./src/app/AppContext"]
		}
	},
	"include": ["src/**/*"]
}
